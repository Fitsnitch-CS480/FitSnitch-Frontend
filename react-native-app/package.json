{
  "name": "fitsnitchapp",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "postversion": "react-native-version --never-amend",
    "android": "npm run tunnel:android && react-native run-android --mode=debug --appIdSuffix=debug",
    "android:dev": "npm run tunnel:android && react-native run-android --mode=dev --appIdSuffix=dev",
    "android:beta": "npm run tunnel:android && react-native run-android --mode=beta",
    "android:prod": "npm run tunnel:android && react-native run-android --mode=release",
    "tunnel:android": "adb reverse tcp:4000 tcp:4000",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle:dev": "cd android && ./gradlew assembleDev",
    "bundle:beta": "cd android && ./gradlew bundleBeta",
    "android:signingReport": "cd android && ./gradlew signingReport"
  },
  "dependencies": {
    "@notifee/react-native": "^7.5.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-firebase/app": "^17.3.2",
    "@react-native-firebase/auth": "^17.3.2",
    "@react-native-firebase/messaging": "^17.3.1",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.4.1",
    "@react-navigation/native": "^6.0.14",
    "@react-navigation/native-stack": "^6.9.2",
    "aws-sdk": "^2.1259.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.6",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "18.2.0",
    "react-native": "0.71.1",
    "react-native-config": "^1.4.11",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-input-spinner": "^1.7.12",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-share": "^8.0.0",
    "react-native-sound": "^0.11.2",
    "react-native-vector-icons": "^9.2.0",
    "react-native-version": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
